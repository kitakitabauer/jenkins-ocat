{
   "manifest_version": 2,
   "name": "JenkinsさんからWebsocketを受け取るやつ2",
   "version": "1.2",
   "description": "Jenkinsさんからの通知を受け取ります",
   "background": {
       "scripts": [
            "js/jquery.min.js",
            "js/notification.js",
            "js/main.js"
       ]
   },
   "icons": {
      "128": "img/jenkins.png",
      "16": "img/jenkins.png",
      "48": "img/jenkins.png"
   },
   "options_page": "html/options.html",
   "permissions": [ "notifications", "background" ]
}
